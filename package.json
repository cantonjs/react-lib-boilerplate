{
  "name": "react-lib-boilerplate",
  "version": "0.0.0",
  "description": "react-lib-boilerplate",
  "repository": "cantonjs/react-lib-boilerplate",
  "scripts": {
    "start": "npm test -- --watchAll",
    "test": "cross-env BABEL_ENV=cjs jest",
    "coverage": "npm test -- --coverage",
    "format": "prettier-eslint --write \"+(src|test)/**/*.js\"",
    "rimraf": "rimraf ./dist ./lib ./es",
    "mkdir": "mkdirp ./dist ./lib ./es",
    "prebuild": "run-s rimraf mkdir",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -d lib --ignore __tests__",
    "build:es": "cross-env BABEL_ENV=es babel src -d es --ignore __tests__",
    "build:umd": "cross-env NODE_ENV=production webpack",
    "build:min": "cross-env NODE_ENV=production webpack --env.minify",
    "build": "run-p build:*",
    "preversion": "npm test && npm run build"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "lib",
    "es",
    "dist"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "author": "cap32",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^3.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.4.2",
    "lodash.camelcase": "^4.3.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "prettier-eslint-cli": "^4.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.5.4",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10"
  }
}
